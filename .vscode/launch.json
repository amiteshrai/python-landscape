{
  "name": "mypy",
  "type": "python",
  "request": "launch",
  "module": "mypy",
  "args": ["${file}"],
  "console": "integratedTerminal",

  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "pythonPath": "${config:python.pythonPath}",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "env": {},
      "envFile": "${workspaceRoot}/.env"
    }
  ]
}
